{
  "name": "tge-server",
  "version": "1.0.0",
  "description": "tge-server",
  "author": "Trustlab",
  "engines": {
    "node": "6.11.1",
    "npm": "3.10.10"
  },
  "scripts": {
    "start": "node ./dist/js/server.js",
    "postinstall": "NODE_ENV=production webpack --config ./webpack/release.js && gulp",
    "dev": "NODE_ENV=dev node ./webpack/dev.js",
    "dev-fast": "NODE_ENV=dev TS_CONFIG=fast node ./webpack/dev.js",
    "watch-server": "node ./node_modules/webpack/bin/webpack.js --watch --config ./webpack/server.js",
    "watch-client": "node ./node_modules/webpack/bin/webpack.js --watch --config ./webpack/client.js",
    "watch-client-fast": "TS_CONFIG=fast node ./node_modules/webpack/bin/webpack.js --watch --config ./webpack/client.js",
    "watch-client-fast-win": "set TS_CONFIG=fast && node ./node_modules/webpack/bin/webpack.js --watch --config ./webpack/client.js",
    "dev-watch": "concurrently --kill-others \"npm run dev\" \"npm run watch-client\"",
    "dev-watch-fast": "concurrently --kill-others \"npm run dev-fast\" \"npm run watch-client-fast\"",
    "dev-win": "set NODE_ENV=dev && node ./webpack/dev.js",
    "dev-fast-win": "set NODE_ENV=dev && set TS_CONFIG=fast && node ./webpack/dev.js",
    "dev-watch-win": "concurrently --kill-others \"npm run dev-win\" \"npm run watch-client\"",
    "dev-watch-fast-win": "concurrently --kill-others \"npm run dev-win-fast\" \"npm run watch-client-fast-win\"",
    "test": "NODE_ENV=test mocha --opts mocha.opts --timeout 10000 && rm -rf test",
    "prod": "NODE_ENV=production IS_LOCAL_HOST=true node ./dist/js/server.js",
    "profile": "NODE_ENV=production webpack --config ./webpack/client.js --profile --json > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ixofoundation/tge-server.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ixofoundation/tge-server/issues"
  },
  "homepage": "https://github.com/ixofoundation/tge-server#readme",
  "dependencies": {
    "@types/react": "15.6.1",
    "@types/react-dom": "15.5.4",
    "@types/react-redux": "4.4.47",
    "@types/react-router": "3.0.5",
    "autoprefixer": "7.1.6",
    "bip39": "2.4.0",
    "body-parser": "1.15.2",
    "bootstrap": "3.3.7",
    "cookie-parser": "1.4.3",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.23.1",
    "del": "2.2.2",
    "ejs": "2.4.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "gulp": "3.9.1",
    "gulp-htmlmin": "3.0.0",
    "gulp-minify-css-names": "1.3.3",
    "gulp-rev": "7.1.2",
    "gulp-sequence": "0.4.6",
    "http-proxy-middleware": "0.16.0",
    "immutable": "3.8.2",
    "json-loader": "0.5.4",
    "mongodb": "2.2.5",
    "node-ensure": "0.0.0",
    "postcss": "6.0.12",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.5",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "15.6.1",
    "react-json-pretty": "^1.7.4",
    "react-jsonschema-form": "0.40.0",
    "react-redux": "4.4.8",
    "react-router": "3.0.5",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "sovrin-did": "1.2.0",
    "style-loader": "0.13.1",
    "ts-loader": "3.1.0",
    "typescript": "2.3.4",
    "typings": "2.1.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "1.14.1"
  },
  "devDependencies": {
    "browser-sync": "2.18.13",
    "copy-webpack-plugin": "4.0.1",
    "dotenv": "2.0.0",
    "ts-node": "3.3.0"
  }
}
